@page "/"

<MudText Typo="Typo.h3" GutterBottom="true">Dashboard</MudText>
<MudText Class="mb-8">Welcome to the Marketplace Admin Portal.</MudText>

<MudGrid>
    <MudItem xs="12" sm="6" md="4">
        <MudPaper Class="d-flex align-center justify-center mud-width-full py-8">
            <div>
                <MudText Typo="Typo.h6">Total Orders</MudText>
                <MudText Typo="Typo.h4">1,234</MudText>
            </div>
        </MudPaper>
    </MudItem>
    <MudItem xs="12" sm="6" md="4">
        <MudPaper Class="d-flex align-center justify-center mud-width-full py-8">
             <div>
                <MudText Typo="Typo.h6">Revenue</MudText>
                <MudText Typo="Typo.h4">$45,678</MudText>
            </div>
        </MudPaper>
    </MudItem>
     <MudItem xs="12" sm="12" md="4">
        <MudPaper Class="d-flex align-center justify-center mud-width-full py-8">
             <div>
                <MudText Typo="Typo.h6">Active Users</MudText>
                <MudText Typo="Typo.h4">567</MudText>
            </div>
        </MudPaper>
    </MudItem>
</MudGrid>
